{
  "expo": {
    "name": "INELIREFI",
    "slug": "inelirefi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "inelirefi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#9b2226",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.colvenda.inelirefi"
    },
    "web": {
  "bundler": "metro",
  "output": "static",
  "favicon": "./assets/images/favicon.png",
  "config": {
    "pwa": {
      "name": "INELIREFI App",
      "shortName": "INELIREFI",
      "description": "Plataforma educativa de INELIREFI",
      "themeColor": "#9b2226",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png"
    }
  }
},
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b1c41ce3-7954-4a03-8d9b-147d0d1440d4"
      }
    }
  }
}